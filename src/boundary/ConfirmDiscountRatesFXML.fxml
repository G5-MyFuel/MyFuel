<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="796.0" prefWidth="1062.0" style="-fx-position-shape: center; -fx-border-color: transparent;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boundary.ConfirmDiscountRatesBoundary">
   <children>
      <Label layoutX="39.0" layoutY="55.0" text="Confirm Discount Rates" textFill="#2d4578">
         <font>
            <Font name="System Bold" size="40.0" />
         </font>
      </Label>
      <TableView fx:id="TableSubscriptionType" layoutX="39.0" layoutY="164.0" onMouseClicked="#TableClicked" prefHeight="253.0" prefWidth="996.0">
         <columns>
            <TableColumn fx:id="SubscriptionTypeColumn" prefWidth="332.0" text="Subscription type" />
            <TableColumn fx:id="CurrentPriceColumnColumn" minWidth="1.0" prefWidth="332.0" text="Current price" />
            <TableColumn fx:id="NewPriceColumn" prefWidth="332.0" text="New price" />
         </columns>
      </TableView>
      <Button fx:id="btnApprovedRates" layoutX="138.0" layoutY="500.0" mnemonicParsing="false" onAction="#handleApprovedRates" prefHeight="42.0" prefWidth="156.0" style="-fx-background-color: #2D4578; -fx-background-radius: 50;" stylesheets="@style.css" text="Approved" />
      <Button fx:id="btnRemoveNewRate" layoutX="635.0" layoutY="500.0" mnemonicParsing="false" onAction="#handleRemoveNewRate" prefHeight="42.0" prefWidth="156.0" style="-fx-background-color: #2D4578; -fx-background-radius: 50;" stylesheets="@style.css" text="Remove" />
      <Label fx:id="NoNewRatePendingLabel" layoutX="39.0" layoutY="137.0" text="*There is no new rate pending approval currently!" textFill="#ff0f02">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
   </children>
</Pane>
